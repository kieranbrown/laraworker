{
  "$schema": "../node_modules/wrangler/config-schema.json",
  "name": "{{APP_NAME}}",
  "main": "worker.ts",
  "compatibility_date": "{{COMPATIBILITY_DATE}}",
  "minify": true,
  "observability": {
    "logs": {
      "enabled": true
    },
    "traces": {
      "enabled": true
    }
  },
  "limits": {
    "cpu_ms": 10000
  },
  "assets": {
    "directory": "./dist/assets",
    "binding": "ASSETS",
    "run_worker_first": ["/build/assets/*"]
  }
}
