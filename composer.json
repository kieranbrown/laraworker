{
    "name": "kieranbrown/laraworker",
    "description": "Run Laravel on Cloudflare Workers via php-cgi-wasm",
    "type": "library",
    "keywords": [
        "laravel",
        "cloudflare",
        "workers",
        "wasm",
        "php-wasm"
    ],
    "license": "MIT",
    "require": {
        "php": "^8.5",
        "illuminate/console": "^11.0|^12.0",
        "illuminate/support": "^11.0|^12.0"
    },
    "require-dev": {
        "laravel/pint": "^1.27",
        "orchestra/testbench": "^10.0",
        "pestphp/pest": "^3.0"
    },
    "autoload": {
        "psr-4": {
            "Laraworker\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Laraworker\\LaraworkerServiceProvider"
            ]
        }
    },
    "config": {
        "sort-packages": true,
        "allow-plugins": {
            "pestphp/pest-plugin": true
        }
    },
    "minimum-stability": "stable",
    "prefer-stable": true
}
